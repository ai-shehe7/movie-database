{
    "version": 2,
    "builds": [
      { "src": "package.json", "use": "@vercel/static-build" },
      { "src": "./index.cjs", "use": "@vercel/node" }
    ],
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/server/index.js" },
      { "source": "(.*)", "destination": "/index.html" }
    ]
  }